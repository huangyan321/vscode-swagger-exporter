{
  "publisher": "antfu",
  "name": "swagger-exporter",
  "displayName": "swagger-exporter",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.10.5",
  "description": "",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/ext-name#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/ext-name"
  },
  "bugs": {
    "url": "https://github.com/antfu/ext-name/issues"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/antfu"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.83.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "title": "swagger-exporter",
      "properties": {
        "swagger-exporter.resources": {
          "type": "string",
          "description": "swagger列表源路径"
        },
        "swagger-exporter.cookie": {
          "type": "string",
          "description": "swagger-api 的cookie"
        },
        "swagger-exporter.tsSavePath": {
          "type": "string",
          "description": "生成的ts文件存放的目录",
          "default": "src/swagger_api/"
        }
      }
    },
    "commands": [
      {
        "command": "swagger-exporter.start",
        "title": "Run Swagger Exporter"
      }
    ],
    "keybindings": [
      {
        "command": "swagger-exporter.start",
        "key": "ctrl+f10",
        "mac": "cmd+f10"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "swagger-exporter",
          "title": "Swagger Exporter",
          "icon": "media/dep.svg"
        }
      ]
    },
    "views": {
      "swagger-exporter": [
        {
          "id": "ApiExplorer",
          "name": "浏览接口",
          "icon": "media/dep.svg",
          "contextualTitle": "Swagger Exporter"
        }
      ]
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.1.1",
    "@antfu/ni": "^0.21.9",
    "@types/node": "^20.9.0",
    "@types/vscode": "^1.84.1",
    "@vscode/vsce": "^2.22.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "esno": "^4.0.0",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "axios": "^1.6.8"
  }
}
